{
  "name": "kickstart-svelte-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run check && vite build",
    "check": "svelte-check --tsconfig ./tsconfig.app.json",
    "lint": "eslint ./src",
    "fmt": "dprint fmt",
    "fmt:check": "dprint check",
    "model:generate": "npx tsx ./scripts/generateModel.ts",
    "model:import": "npx tsx ./scripts/importProject.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@kontent-ai/delivery-sdk": "^16.4.0",
    "@kontent-ai/rich-text-resolver": "^3.0.2",
    "@portabletext/svelte": "^3.0.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/svelte-query": "^6.0.18",
    "dotenv": "^17.2.3",
    "svelte": "^5.49.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@eslint/js": "^9.39.2",
    "@kontent-ai/biome-config": "^0.7.0",
    "@kontent-ai/data-ops": "^2.5.0",
    "@kontent-ai/model-generator": "^9.1.1",
    "@tsconfig/svelte": "^5.0.6",
    "@types/node": "^25.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^17.2.0",
    "svelte-check": "^4.3.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1"
  }
}
